{
    "name":"Ouakamapse extension",
    "description":"Plugin google meet",
    "version":"0.1.0",
    "manifest_version":2,
    "icons":{
        "16":"O-16x16.png",
        "32":"O-32x32.png",
        "48":"O-48x48.png",
        "128":"O-128x128.png"
    },
    "content_scripts":[
        {
            "matches":["https://meet.google.com/*"],
            "js":["connexion.js"]
        }       
    ],
    "background": {
        "scripts":["./connexion.js"],
        "persistent": false
    },
    "options_page":"./options.html",
    "browser_action":{
        "default_popup":"./connexion.html"
    },
    "permissions":[
        "tabs",
        "https://meet.google.com/*"
    ]
}